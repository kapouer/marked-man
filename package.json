{
  "name": "marked-man",
  "version": "2.0.0",
  "description": "wrapper adding manpage output to 'marked', inspired by 'ronn'",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "marked-man": "./bin/marked-man.js"
  },
  "files": [
    "bin/",
    "src/"
  ],
  "man": [
    "./man/marked-man.1"
  ],
  "scripts": {
    "test": "node ./test/compare"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kapouer/marked-man.git"
  },
  "keywords": [
    "markdown",
    "roff",
    "ronn"
  ],
  "author": "Jérémy Lal <kapouer@melix.org>",
  "license": "MIT",
  "dependencies": {
    "marked": "^12.0.1",
    "read-pkg-up": "^11.0.0"
  },
  "devDependencies": {
    "@kapouer/eslint-config": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "@kapouer/eslint-config",
    "overrides": [
      {
        "files": [
          "bin/*.js",
          "src/*.js",
          "test/*.js"
        ],
        "parserOptions": {
          "sourceType": "module"
        }
      }
    ]
  }
}
