{
  "name": "marked-man",
  "version": "1.0.0",
  "description": "wrapper adding manpage output to 'marked', inspired by 'ronn'",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "marked-man": "./bin/marked-man.js"
  },
  "man": [
    "./man/marked-man.1"
  ],
  "scripts": {
    "test": "node ./test/compare"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kapouer/marked-man.git"
  },
  "keywords": [
    "markdown",
    "roff",
    "ronn"
  ],
  "author": "Jérémy Lal <kapouer@melix.org>",
  "license": "MIT",
  "peerDependencies": {
    "marked": "^4.2.0"
  },
  "devDependencies": {
    "@kapouer/eslint-config": "^1.8.0"
  },
  "eslintConfig": {
    "extends": "@kapouer/eslint-config",
    "overrides": [
      {
        "files": [
          "src/**.js", "test/**.js"
        ],
        "env": {
          "es6": true, "node": true
        }
      }
    ]
  }
}
